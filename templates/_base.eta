<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <meta name="robots" content="index, follow">
  <link type="text/plain" rel="author" href="https://kvaapagarrot.com/humans.txt">

  <link rel="preconnect" href="https://fonts.googleapis.com" crossorigin>
  <link rel="dns-prefetch" href="https://fonts.googleapis.com">
  <link rel="dns-prefetch" href="https://fonts.gstatic.com">
  <link rel="dns-prefetch" href="https://www.googletagmanager.com">
  <link rel="dns-prefetch" href="https://mc.yandex.ru">

  <title><%= it.title ? it.title : 'Margarita Kvaapagarrot' %></title>

  <meta name="title" content="<%= it.title ? it.title : 'Margarita Kvaapagarrot' %>">
  <meta name="description" content="Margarita Kvaapagarrot. Siberian artist">
  <meta name="keywords" content="Margarita,Kvaapagarrot,Margo,art">
  <meta name="author" content="Margarita Kvaapagarrot">

  <link rel="icon" href="/favicon.svg" type="image/svg+xml">
  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
  <link type="application/json" rel="manifest" href="/site.webmanifest">
  <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#333333">
  <meta name="apple-mobile-web-app-title" content="Kvaapagarrot">
  <meta name="application-name" content="Kvaapagarrot">
  <meta name="msapplication-TileColor" content="#000000">
  <meta name="theme-color" content="#000000">

  <!-- Open Graph / Facebook -->
  <meta property="og:type" content="website">
  <meta property="og:url" content="https://kvaapagarrot.com/">
  <meta property="og:title" content="<%= it.title ? it.title : 'Margarita Kvaapagarrot' %>">
  <meta property="og:description" content="Siberian artist">
  <meta property="og:image" content="https://kvaapagarrot.com/img/open-graph-cover.jpg">

  <!-- Twitter -->
  <meta property="twitter:card" content="summary_large_image">
  <meta property="twitter:url" content="https://kvaapagarrot.com/">
  <meta property="twitter:title" content="<%= it.title ? it.title : 'Margarita Kvaapagarrot' %>">
  <meta property="twitter:description" content="Siberian artist">
  <meta property="twitter:image" content="https://kvaapagarrot.com/img/open-graph-cover.jpg">

  <link rel="preload" href="https://fonts.googleapis.com/css2?family=Lora:wght@400;500&family=Raleway:wght@350&display=swap"
        as="style" onload="this.onload=null;this.rel='stylesheet'" media="screen,print">
  <noscript><link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Lora:wght@400;500&family=Raleway:wght@350&display=swap" media="screen,print"></noscript>

  <link rel="stylesheet" href="/dist/main.css">
</head>
<body role="document">
  <%~ it.body %>
  <%~ includeFile('./_telemetry', it) %>
</body>
</html>
