{
  "name": "kvaapagarrot.com",
  "version": "1.0.0",
  "main": "dist/main.js",
  "author": "Stepan Legachev <gitlog@sib.li>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=development rollup -c",
    "build-frontend": "rollup -c rollup.frontend.config.js && rm public/dist/main.js",
    "deploy": "gcloud app deploy app.yaml --project kvaapagarrot --quiet",
    "gcp-build": "NODE_ENV=production rollup -c",
    "lint": "eslint '**/*.ts'",
    "slint": "stylelint '**/*.pcss'",
    "start": "node -r source-map-support/register dist/main",
    "static": "tsc -p scripts && node -r source-map-support/register scripts/dist/make-static-handlers",
    "watch": "NODE_ENV=development rollup -c -w"
  },
  "dependencies": {
    "eta": "^1.12.1",
    "fast-glob": "^3.2.4",
    "fastify": "^3.9.2",
    "fastify-static": "^3.3.1",
    "mnemonist": "^0.38.1",
    "point-of-view": "^4.7.0",
    "source-map-support": "^0.5.19",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-run": "^2.0.2",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "autoprefixer": "^9.0.0",
    "chokidar": "^3.4.3",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "js-yaml": "^4.0.0",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^4.1.3"
  }
}
